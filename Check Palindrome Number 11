import java.io.*;
import java.util.Scanner;
public class Main{
 public static void main(String[]args) {
     Scanner s=new Scanner(System.in);
     int N=s.nextInt();
     int original=N;
     int reversed=0;
     while (N>0){
         int digit=N%10;
         reversed=reversed*10+digit;
         N=N/10;
     }
     if (original==reversed){
         System.out.println("Palindrome");
     }
         else
         {
             System.out.println("Not Palindrome");
         }
     }
         
         
     
 }
    
