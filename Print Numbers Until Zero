import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner (System.in);
        int number;
        do {
            number =s.nextInt();
            if (number !=0){
                System.out.println(number);
            
            }
        }
            while (number !=0);
    }
}
            
        
    
