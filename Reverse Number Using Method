import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner s=new Scanner(System.in);
            
        int N=s.nextInt();
          int reversed=0;
        while(N>0){
            int digit=N%10;
            reversed=reversed*10+digit;
            N=N/10;
        }
      System.out.println(reversed) ; 
        
    }
}
