import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int N=s.nextInt();
        int factorial=1;
        for (int i=1;i<=N;i++){
            factorial=factorial*i;}
        {
            System.out.println((factorial));
        }
            
        
        
        
    }
}
