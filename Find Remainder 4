import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        
        int a;
        int b;
        a=s.nextInt();
        b=s.nextInt();
        
        
        if(a%b==0){
            System.out.print(0);
            }
        else{
            System.out.println(2);
        }
    }
}
