import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a[]=new int[100];
        int N=s.nextInt();
        int sum=0;
        
        for (int i=0;i<N;i++)
        {
            a[i]=s.nextInt();
            sum=sum+a[i];
        }
        
            System.out.println(sum);
        
        }
}
