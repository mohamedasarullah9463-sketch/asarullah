import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
         int a=s.nextInt();
        int b=s.nextInt();
        int c=s.nextInt();
        int largest;
        if(a>=b&& a>=c)
            System.out.println(a);
        if(b>=a && b>=c)
            System.out.println(b);
        else
        {System.out.println(c);}
    }
}
